<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="win.prashant.login">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="Reise"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginSignup"
            android:label="@string/title_activity_login_signup" />
        <activity
            android:name=".Register"
            android:label="@string/title_activity_register" />
        <activity
            android:name=".RateUs"
            android:label="@string/title_activity_rate_us" />
        <activity
            android:name=".Share"
            android:label="@string/title_activity_share" />
        <activity android:name=".Options" />
        <activity
            android:name=".Helpfeedback"
            android:label="@string/title_activity_helpfeedback" />
        <activity android:name=".Delete"></activity>
    </application>

</manifest>